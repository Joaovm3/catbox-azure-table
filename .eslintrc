{
    "env":{
        "node": true
    },
    "rules": {
        "eol-last": 2,
        "strict": [2, "global"],
        "quotes": [2, "single"],
        "no-underscore-dangle": 0,
        "camelcase": 0,
        "new-cap": 0,
        "no-octal-escape": 0,
        "key-spacing": 0,
        "no-shadow": 0
    }
}